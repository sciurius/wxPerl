OUTPUT
O_WXOBJECT
    _object_2_sv( $arg, $var );

O_NON_WXOBJECT
    _non_object_2_sv( $arg, $var, ${(my $ntt=$ntype)=~s/^Wx_(.*)Ptr$/wxPl$1Name/g;\${ntt}} );

INPUT
O_WXOBJECT
    $var = ($type) _sv_2_object( $arg, ${(my $ntt=$ntype)=~s/^Wx_(.*)Ptr$/wxPl$1Name/g;\${ntt}} );

O_NON_WXOBJECT
    $var = ($type) _sv_2_object( $arg, ${(my $ntt=$ntype)=~s/^Wx_(.*)Ptr$/wxPl$1Name/g;\${ntt}} );

O_POINT
    $var = _sv_2_wxpoint( $arg );

O_SIZE
    $var = _sv_2_wxsize( $arg );

O_COLOUR
    $var = *($type *) _sv_2_object( $arg, ${(my $ntt=$ntype)=~s/^Wx_(.*)(?:Ptr)?$/wxPl$1Name/g;\${ntt}} );
